<div class="signuppage">

<div class="row-fluid">
	<div class="span4 offset4">
		<div class="signupwrapper">
	

						<div class="usersignupemail">

							<div class="row-fluid">
									<div class="signuptop">
										Sign in
									</div>	
							</div>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
<div class="row-fluid">
  <%= f.email_field :email, :placeholder => "Email Address", :class => "usersignupemail2" %>
</div>

<div class="row-fluid">
  <%= f.password_field :password, :placeholder => "Password", :class => "usersignupemail2" %>
</div>

  <div class="row-fluid">
  <% if devise_mapping.rememberable? -%>
    <%= f.check_box :remember_me %> Remember me
  <% end %>
</div>
<br>
<div class="row-fluid">
<%= f.submit "Go!", :class => "usersignupsubmit" %>
<% end %>
</div>

<%= render "devise/shared/links" %>

						</div>
		</div>
	</div>
</div>

</div>